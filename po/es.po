#: usr/lib/tuquito/tuquito-upgrade-manager/manager.py:44
#: usr/lib/tuquito/tuquito-upgrade-manager/manager.py:78
#: usr/lib/tuquito/tuquito-upgrade-manager/manager.py:88
msgid "Upgrade Tuquito"
msgstr "Actualizar Tuquito"

#: usr/lib/tuquito/tuquito-upgrade-manager/manager.py:70
msgid "An error occurred during the upgrade: "
msgstr "Ha ocurrido un error durante la actualización: "

#: usr/lib/tuquito/tuquito-upgrade-manager/manager.py:79
#, python-format
msgid "<big><b>You have installed %s</b></big>"
msgstr "<big><b>Tienes instalado %s</b></big>"

#: usr/lib/tuquito/tuquito-upgrade-manager/manager.py:80
#, python-format
msgid "Already available <b>%s</b>!"
msgstr "¡Ya está disponible <b>%s</b>!"

#: usr/lib/tuquito/tuquito-upgrade-manager/manager.py:81
msgid "Start upgrade"
msgstr "Iniciar actualización"

#: usr/lib/tuquito/tuquito-upgrade-manager/manager.py:82
msgid "Only download the new ISO image"
msgstr "Solo descargar la nueva imagen ISO"

#: usr/lib/tuquito/tuquito-upgrade-manager/manager.py:83
msgid "See the new features on the web"
msgstr "Ver las nuevas características en la web"

#: usr/lib/tuquito/tuquito-upgrade-manager/manager.py:84
msgid "I don't want to upgrade my Tuquito"
msgstr "No quiero actualizar mi Tuquito"

#: usr/lib/tuquito/tuquito-upgrade-manager/manager.py:89
msgid "<big><b>Information</b></big>"
msgstr "<big><b>Información</b></big>"

#: usr/lib/tuquito/tuquito-upgrade-manager/manager.py:90
#, python-format
msgid ""
"You've already installed the latest version of Tuquito.\n"
"Distribution: <b>%s</b>"
msgstr ""
"Tienes instalada la última versión de Tuquito.\n"
"Distribución: <b>%s</b>"

#: usr/lib/tuquito/tuquito-upgrade-manager/manager.py:112
msgid "<big><b>Atention</b></big>"
msgstr "<big><b>Atención</b></big>"

#: usr/lib/tuquito/tuquito-upgrade-manager/manager.py:113
msgid ""
"To update Tuquito then you can go to:\n"
"<i>Menu»System»Administration»Upgrade Manager</i>"
msgstr ""
"Para actualizar Tuquito luego, puede ir a:\n"
"<i>Menú»Sistema»Administración»Tuquito Upgrade Manager</i>"

#: usr/lib/tuquito/tuquito-upgrade-manager/manager.py:129
msgid "Connecting to server..."
msgstr "Conectando con el servidor..."

#: usr/lib/tuquito/tuquito-upgrade-manager/manager.py:157
msgid ""
"Error connecting to server.\n"
"Try again later."
msgstr ""
"Error de conexión con el servidor.\n"
"Inténtalo nuevamente más tarde."
